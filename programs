Q- Fernit to celcius

#include <stdio.h>

int main()
{
    int a,b;

    printf("enter Fernit\n");
    scanf(
        "%d",&a);
        printf("clecius to f=%d\n",b=(a*1.8) + 32);

    return 0;
}

-------------------------------------------------------
Q- Celcius to Fernit

#include <stdio.h>

int main()
{
    int a,b;

    printf("enter celcius\n");
    scanf(
        "%d",&a);
        printf("fernit to c=%d\n",b=((a- 32)*5)/9);

    return 0;
}

-------------------------------------------------------

Q-Power and Sqrt calculation

#include <stdio.h>
#include <math.h>

int main ()
{
  double a, b;

  printf ("enter x to y \n");
  scanf ("%lf%lf", &a,&b);
  printf ("power%lf\n", pow (a, b));
  return 0;
}

