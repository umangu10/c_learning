Q- airthametic operation


#include <stdio.h>

int main()
{
    int a, b ,sum;
    printf("enter no\n");
    scanf(
        "%d%d",&a,&b);
        printf("%d\n",sum=a+b);
        printf("%d\n",sum=a-b);
        printf("%d\n",sum=a/b);
        printf("%d\n",sum=a%b);
        printf("%d\n",sum=a*b);
    return 0;

}

